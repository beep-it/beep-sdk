{
  "name": "@beep-it/cli",
  "version": "0.1.0-beta.1",
  "description": "CLI for scaffolding and managing BEEP MCP servers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "beep": "./dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts",
    "dev": "tsup src/index.ts --format cjs --dts --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "prepare": "pnpm build",
    "prepack": "pnpm -w --filter @beep-it/sdk-core run pack && pnpm build",
    "pack": "npm pack"
  },
  "keywords": [
    "beep",
    "cli",
    "mcp",
    "scaffold",
    "payment",
    "sdk"
  ],
  "author": "BEEP Engineering",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/beep-it/beep-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/beep-it/beep-sdk/issues"
  },
  "homepage": "https://github.com/beep-it/beep-sdk",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@beep-it/sdk-core": "workspace:*",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "tsup": "^8.2.2",
    "typescript": "^5.5.3"
  }
}
