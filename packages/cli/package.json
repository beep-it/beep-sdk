{
  "name": "@beep/cli",
  "version": "0.1.0",
  "private": true,
  "description": "CLI for scaffolding and managing BEEP MCP servers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "beep": "./dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts",
    "dev": "tsup src/index.ts --format cjs --dts --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "prepare": "pnpm build",
    "prepack": "pnpm -w --filter @beep/sdk-core run pack && pnpm build",
    "pack": "npm pack"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@beep/sdk-core": "workspace:*",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "tsup": "^8.2.2",
    "typescript": "^5.5.3"
  }
}
