{
  "name": "beep-sdk",
  "version": "0.1.0",
  "description": "Official TypeScript SDK for the BEEP Payment and Invoice Server",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "beep",
    "sdk",
    "payment",
    "invoice",
    "solana",
    "mcp"
  ],
  "author": "BEEP Engineering",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.14.10",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "tsup": "^8.2.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "axios": "^1.7.2"
  }
}
